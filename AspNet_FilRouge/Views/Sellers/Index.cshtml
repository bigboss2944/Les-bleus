@model IEnumerable<Entities.Seller>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Seller - Menu</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Password)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Shop.Nameshop)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Role.Name)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.IdSeller }) |
        @Html.ActionLink("Details", "Details", new { id = item.IdSeller }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.IdSeller })
    </td>
</tr>
    }

</table>

<div>
    @Html.ActionLink("Back", "../Home/Index")
</div>